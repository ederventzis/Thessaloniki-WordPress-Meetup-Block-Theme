	"styles": {
		"typography": {
			"fontFamily": "Open Sans"
		},
		"color": {
			"text": "var(--wp--preset--color--grey-500)"
		},
		"border": {
			"width": "1px",
			"color": "var(--wp--preset--color--grey-200)",
			"radius": "5px"
		},
		"blocks": {
			"core/navigation-link": {
				"color": {
					"text": "var(--wp--preset--color--grey-900)"
				},
				"typography": {
					"fontSize": "0.875rem",
					"fontWeight": "500"
				},
				"spacing": {
					"margin": {
						"left": "1rem",
						"right": "1rem"
					}
				},
				"border": {
					"radius": "6px"
				},
				"css": ":last-of-type { padding: 0.5rem 1rem; background: var(--wp--preset--color--red-100); color: var(--wp--preset--color--red); }"
			},
			"core/image": {
				"elements": {
					"caption": {
						"typography": {
							"fontSize": "var(--wp--preset--font-size--xs)",
							"fontStyle": "italic"
						}
					},
					"link": {
						"color": {
							"text": "var(--wp--preset--color--teal)"
						}
					}
				}
			},
			"core/separator": {
				"border": {
					"color": "var(--wp--preset--color--grey-100)"
				}
			},
			"core/button": {
				"spacing": {
					"padding": {
						"top": "1rem",
						"bottom": "1rem",
						"left": "3rem",
						"right": "3rem"
					}
				},
				"border": {
					"radius": "6px"
				},
				"color": {
					"background": "var(--wp--preset--color--red)",
					"text": "var(--wp--preset--color--white)"
				},
				"variations": {
					"outline": {
						"spacing": {
							"padding": {
								"top": "1rem",
								"bottom": "1rem",
								"left": "3rem",
								"right": "3rem"
							}
						}
					}
				}
			},
			"core/social-link": {
				"elements": {
					"link": {
						"color": {
							"background": "var(--wp--preset--color--red-100)",
							"text": "var(--wp--preset--color--red) !important"
						},
						"border": {
							"radius": "100%"
						}
					}
				}
			}
		},
		"elements": {
			"heading": {
				"color": {
					"text": "var(--wp--preset--color--grey-900)"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "4rem"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "3rem"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "2.25rem"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "1.5rem"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "1.25rem"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "1.125rem"
				}
			}
		}
